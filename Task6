function divideNumbers(num1, num2) {
    if (num2 == 0) {
        throw new Error("Cannot divide by zero, Kindly enter a different number");
    }else{
    return num1 / num2;
    }
}

try {
    let result = divideNumbers(10, 3);
    console.log(result);
} catch (error) {
    console.error(error.message);
}
